{
  "name": "@iac/ui",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev:styles": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "dev": "tsup --watch",
    "test": "vitest run",
    "build:styles": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "build": "tsup"
  },
  "exports": {
    "./styles.css": "./dist/styles.css",
    "./button": {
      "types": "./dist/button.d.ts",
      "import": "./dist/button.js",
      "require": "./dist/button.cjs"
    },
    "./popover": {
      "types": "./dist/popover.d.ts",
      "import": "./dist/popover.js",
      "require": "./dist/popover.cjs"
    },
    "./multi-select": {
      "types": "./dist/multi-select.d.ts",
      "import": "./dist/multi-select.js",
      "require": "./dist/multi-select.cjs"
    },
    "./loading": {
      "types": "./dist/loading.d.ts",
      "import": "./dist/loading.js",
      "require": "./dist/loading.cjs"
    },
    "./chip": {
      "types": "./dist/chip.d.ts",
      "import": "./dist/chip.js",
      "require": "./dist/chip.cjs"
    },
    "./input": {
      "types": "./dist/input.d.ts",
      "import": "./dist/input.js",
      "require": "./dist/input.cjs"
    },
    "./select": {
      "types": "./dist/select.d.ts",
      "import": "./dist/select.js",
      "require": "./dist/select.cjs"
    },
    "./message": {
      "types": "./dist/message.d.ts",
      "import": "./dist/message.js",
      "require": "./dist/message.cjs"
    },
    "./card": {
      "types": "./dist/card.d.ts",
      "import": "./dist/card.js",
      "require": "./dist/card.cjs"
    },
    "./typography": {
      "types": "./dist/typography.d.ts",
      "import": "./dist/typography.js",
      "require": "./dist/typography.cjs"
    },
    "./badge": {
      "types": "./dist/badge.d.ts",
      "import": "./dist/badge.js",
      "require": "./dist/badge.cjs"
    },
    "./tabs": {
      "types": "./dist/tabs.d.ts",
      "import": "./dist/tabs.js",
      "require": "./dist/tabs.cjs"
    }
  },
  "dependencies": {
    "@radix-ui/react-popover": "^1.1.14",
    "@tailwindcss/postcss": "^4.1.11",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.6",
    "ldrs": "^1.1.7",
    "lucide-react": "^0.525.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@iac/tailwind-config": "workspace:*",
    "@iac/typescript-config": "workspace:*",
    "@tailwindcss/cli": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4.1.11",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
